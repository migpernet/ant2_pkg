<?xml version="1.0"?>

<launch>

  <!--load the empty world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch" >
    <arg name="paused" value="true" />
  </include>
  <!--carrega o arquivo urdf como nÃ³-->
  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-file $(find ant2_pkg)/urdf/ur5_jnt_pos_ctrl.urdf -urdf -x 0 -y 0 -z 0.1 -model ur5 -J shoulder_lift_joint -1.5 -J elbow_joint 1.0" />

  <include file="$(find ant2_pkg)/launch/ur5_joint_position_control.launch" />

</launch>